<div class="col-md-4">
  <div class="tweets">
    <div class="panel panel-default">
      <table class="table">
        <tbody>
          {{#each model as |tweet|}}
            <tr>
              <td>
                <h3>{{tweet.username}}</h3>
                <img {{bindAttr src="tweet.userProfileImage"}}>
              </td>
              <td style="width:100%;">
                {{tweet.body}}
              </td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>
